import cv2
#Read the image
img1=cv2.imread("C://Users//Mohini11//OneDrive//Desktop//Pics//SKF//IMG_20221016_101808.jpg")
img1=cv2.resize(img1,(1200,900 ))#Width Height
img1=cv2.flip(img1,-1)
#Showing The image
cv2.imshow("Original ",img1)
#waiting using waitkey method
cv2.waitKey(5000)#if u give value as 0 then it remain still any key is pressed
#time is in milisecond
cv2.destroyAllWindows()# Not necessary
#img is in array formate we can change it by changing the array size
print(img1)
